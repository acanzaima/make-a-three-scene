{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*", "src/assets/iconfont/iconfont.js"],
  "compilerOptions": {
    "allowJs": true,
    "composite": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "strictPropertyInitialization": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
